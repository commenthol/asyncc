    "clean": "rimraf lib docs coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "minami": "^1.1.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {}
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
