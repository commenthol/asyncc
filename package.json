{
  "name": "asyncc",
  "version": "0.1.0-0",
  "description": "Just asynchronous patterns",
  "main": "lib",
  "jsnext:main": "src",
  "keywords": [
    "async",
    "callback"
  ],
  "homepage": "https://github.com/commenthol/asyncc",
  "bugs": {
    "url": "https://github.com/commenthol/asyncc/issues"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/asyncc.git"
  },
  "scripts": {
    "dist": "npm run clean && npm run lint && npm run build && npm test && npm version",
    "test": "npm run mocha",
    "mocha": "mocha",
    "build": "babel -d lib src",
    "doc": "jsdoc -c scripts/jsdoc/jsdoc.json",
